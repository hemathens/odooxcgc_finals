# Frontend environment variables for Vite
# Copy this file to .env.local and set your values

# API origin (scheme+host) where your backend runs (do not include trailing slash)
# Default: http://localhost:8000
VITE_API_ORIGIN=http://localhost:8000

# Google Identity Services Client ID (NOT secret)
VITE_GOOGLE_CLIENT_ID={{GOOGLE_CLIENT_ID}}
